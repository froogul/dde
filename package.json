{
  "name": "dexter_dev_env",
  "productName": "dexter_dev_env",
  "version": "2.5.6",
  "release_date": "Sep 6, 2018",
  "description": "Dexter Development Environment",
  "author": "Fry",
  "license": "GPL-3.0",
  "main": "main.js",
  "scripts": {
    "test": "dependency-check ./package.json",
    "start": "electron main",
    "rebuild": "electron-rebuild",
    "build": "build --dir",
    "dist": "build",
    "appveyor": "build.cmd",
    "clean": "rimraf dist"
  },
  "keywords": [
    "robot",
    "Dexter",
    "Haddington"
  ],
  "dependencies": {
    "awesomplete": "^1.1.2",
    "electron-context-menu": "^0.8.0",
    "eslint": "^3.14.1",
    "esprima": "^3.1.3",
    "jpeg-js": "^0.3.3",
    "jquery": "^3.1.1",
    "opencv.js": "^1.2.1",
    "ping": "^0.2.2",
    "request": "^2.79.0",
    "rotating-calipers": "0.0.0",
    "semver": "^5.4.1",
    "serialport": "^7.0.2",
    "three-stl-loader": "^1.0.6",
    "webmidi": "^2.2.0",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "dependency-check": "^2.8.0",
    "electron": "^1.4.13",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.5.7",
    "rimraf": "^2.6.1"
  },
  "build": {
    "appId": "com.haddington.dde",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development;Education;Robotics"
    }
  }
}

"electron-rebuild": "electron-rebuild -w opencv4nodejs"
